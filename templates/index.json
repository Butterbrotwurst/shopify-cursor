":"normal","alignment":"left","type_preset":"custom","font":"var(--font-accent--family)","font_size":"1.5rem","line_height":"normal","letter_spacing":"normal","case":"none","wrap":"pretty","color":"","background":false,"background_color":"#00000026","corner_radius":0,"padding-block-start":0,"padding-block-end":9,"padding-inline-start":0,"padding-inline-end":0},"blocks":{}},"custom_liquid_3FB9fx":{"type":"custom-liquid","name":"t:names.custom_liquid","disabled":true,"settings":{"custom_liquid":"<div id=\"addr-cta\" class=\"addr-cta\">\n  <label for=\"addr-cta-input\" class=\"visually-hidden\">Adresse<\/label>\n  <div class=\"addr-input-wrap\">\n    <input id=\"addr-cta-input\" type=\"text\" inputmode=\"text\" autocomplete=\"off\"\n           placeholder=\"Adresse eingeben (z. B. Musterstraße 12, Berlin)\">\n    <ul id=\"addr-suggest\" class=\"addr-suggest\" role=\"listbox\" aria-label=\"Adressvorschläge\"><\/ul>\n  <\/div>\n  <button id=\"addr-cta-btn\" type=\"button\" aria-label=\"Weiter zur Konfiguration\">\n    Weiter <span class=\"arrow\" aria-hidden=\"true\">→<\/span>\n  <\/button>\n<\/div>\n\n<style>\n  :root{\n    --addr-bg:#d6e2df; --addr-input-bg:#fff; --addr-border:#cfd8d6;\n    --addr-text:#1a1a1a; --addr-muted:#6b6f72; --addr-cta:#111; --addr-cta-text:#fff;\n    --addr-hover:rgba(0,0,0,.05);\n  }\n  .addr-cta{display:flex;gap:10px;align-items:center;padding:14px;border-radius:16px;\n    background:var(--addr-bg);max-width:780px;margin:24px auto;box-shadow:0 6px 14px rgba(16,24,40,.06)}\n  .addr-input-wrap{position:relative;flex:1}\n  .addr-cta input{width:100%;height:48px;padding:0 16px;border:1px solid var(--addr-border);\n    border-radius:9999px;background:var(--addr-input-bg);color:var(--addr-text);font-size:16px;outline:none}\n  .addr-cta input::placeholder{color:var(--addr-muted)}\n  .addr-cta button{height:48px;padding:0 18px;border:0;border-radius:9999px;background:var(--addr-cta);\n    color:var(--addr-cta-text);font-weight:600;cursor:pointer;display:inline-flex;align-items:center;gap:8px;\n    box-shadow:0 2px 6px rgba(17,17,17,.15);transition:transform .06s ease, box-shadow .2s ease}\n  .addr-cta button:hover{transform:translateY(-1px);box-shadow:0 6px 16px rgba(17,17,17,.18)}\n  .addr-cta .arrow{font-size:18px}\n  @media (max-width:640px){.addr-cta{flex-direction:column;align-items:stretch;gap:12px}\n    .addr-cta button{justify-content:center}}\n  .addr-suggest{position:absolute;left:0;right:0;top:calc(100% + 6px);background:#fff;border:1px solid var(--addr-border);\n    border-radius:12px;list-style:none;margin:0;padding:6px 0;box-shadow:0 10px 22px rgba(16,24,40,.14);z-index:50;display:none;max-height:280px;overflow:auto}\n  .addr-suggest.show{display:block}\n  .addr-suggest li{padding:10px 14px;cursor:pointer}\n  .addr-suggest li:hover,.addr-suggest li[aria-selected=\"true\"]{background:var(--addr-hover)}\n  .visually-hidden{position:absolute!important;height:1px;width:1px;overflow:hidden;clip:rect(1px,1px,1px,1px);white-space:nowrap}\n<\/style>\n\n<script>\n(function(){\n  var input = document.getElementById('addr-cta-input');\n  var btn   = document.getElementById('addr-cta-btn');\n  var list  = document.getElementById('addr-suggest');\n  var activeIndex = -1;            \/\/ Tastaturnavigation\n  var items = [];\n  var debounceTimer;\n  var lastController;\n\n  function setSuggestions(results){\n    list.innerHTML = '';\n    items = results.map(function(r){\n      var li = document.createElement('li');\n      li.setAttribute('role','option');\n      li.textContent = r.display_name;\n      li.addEventListener('mousedown', function(e){ \/\/ mousedown, damit blur das Menü nicht schließt, bevor wir setzen\n        e.preventDefault();\n        choose(r.display_name);\n      });\n      list.appendChild(li);\n      return li;\n    });\n    activeIndex = -1;\n    list.classList.toggle('show', items.length>0);\n  }\n\n  function choose(text){\n    input.value = text;\n    hide();\n    go();\n  }\n\n  function hide(){ list.classList.remove('show'); }\n\n  function fetchSuggestions(q){\n    if(lastController){ lastController.abort(); }\n    lastController = new AbortController();\n\n    var url = 'https:\/\/nominatim.openstreetmap.org\/search?format=jsonv2&q='\n              + encodeURIComponent(q) + '&limit=6&addressdetails=1';\n    fetch(url, {\n      signal: lastController.signal,\n      headers: { 'Accept-Language': 'de,en;q=0.8' }\n    })\n    .then(function(res){ return res.json(); })\n    .then(function(json){\n      setSuggestions(json || []);\n    })\n    .catch(function(err){ if(err.name!=='AbortError'){ setSuggestions([]); }});\n  }\n\n  input.addEventListener('input', function(){\n    var q = input.value.trim();\n    clearTimeout(debounceTimer);\n    if(!q || q.length < 3){ hide(); return; }\n    debounceTimer = setTimeout(function(){ fetchSuggestions(q); }, 250);\n  });\n\n  input.addEventListener('keydown', function(e){\n    if(!list.classList.contains('show')){ if(e.key==='Enter'){ e.preventDefault(); go(); } return; }\n    if(e.key === 'ArrowDown'){\n      e.preventDefault();\n      activeIndex = Math.min(activeIndex + 1, items.length - 1);\n      updateActive();\n    }else if(e.key === 'ArrowUp'){\n      e.preventDefault();\n      activeIndex = Math.max(activeIndex - 1, 0);\n      updateActive();\n    }else if(e.key === 'Enter'){\n      e.preventDefault();\n      if(activeIndex >= 0 && items[activeIndex]) { choose(items[activeIndex].textContent); }\n      else { go(); }\n    }else if(e.key === 'Escape'){ hide(); }\n  });\n\n  function updateActive(){\n    items.forEach(function(li,i){\n      li.setAttribute('aria-selected', i===activeIndex ? 'true' : 'false');\n      if(i===activeIndex){ li.scrollIntoView({block:'nearest'}); }\n    });\n  }\n\n  input.addEventListener('focus', function(){\n    if(items.length>0) list.classList.add('show');\n  });\n  input.addEventListener('blur', function(){\n    \/\/ kleines Delay, damit Klicks auf Vorschlag noch registriert werden\n    setTimeout(hide, 120);\n  });\n\n  function go(){\n    var addr = (input.value || '').trim();\n    if(!addr){ input.focus(); return; }\n    var targetPath = '\/pages\/konfigurator'; \/\/ ggf. anpassen\n    var url = targetPath + '?addr=' + encodeURIComponent(addr);\n    window.location.href = url;\n  }\n\n  btn.addEventListener('click', go);\n})();\n<\/script>"},"blocks":{}},"button_ncJNB9":{"type":"button","name":"t:names.button","settings":{"label":"Eigene 3D Karte erstellen!","link":"shopify:\/\/pages\/map-creator","open_in_new_tab":true,"style_class":"button","width":"fit-content","custom_width":100,"width_mobile":"fit-content","custom_width_mobile":100},"blocks":{}},"text_m9AcWC":{"type":"text","settings":{"text":"<h1>Our Newest Arrivals <\/h1>","width":"100%","max_width":"normal","alignment":"left","type_preset":"h2","font":"var(--font-primary--family)","font_size":"","line_height":"normal","letter_spacing":"normal","case":"none","wrap":"pretty","color":"","background":false,"background_color":"#00000026","corner_radius":0,"padding-block-start":9,"padding-block-end":0,"padding-inline-start":0,"padding-inline-end":0},"blocks":{}}},"block_order":["text_nptTQe","custom_liquid_3FB9fx","button_ncJNB9","text_m9AcWC"]},"text_ebNxhf":{"type":"text","name":"Text","settings":{"text":"<p>Bring order to the chaos. When all your tech essentials have a dedicated spot on your desk, you don’t have to waste any time searching for them. <\/p>","width":"fit-content","max_width":"none","alignment":"left","type_preset":"rte","font":"var(--font-primary--family)","font_size":"","line_height":"normal","letter_spacing":"normal","case":"none","wrap":"pretty","color":"","background":false,"background_color":"#00000026","corner_radius":0,"padding-block-start":0,"padding-block-end":0,"padding-inline-start":0,"padding-inline-end":0},"blocks":{}}},"block_order":["group_U86Qji","text_ebNxhf"]}},"block_order":["group_mATTUY"],"name":"t:names.hero","settings":{"media_type_1